  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!p-retry|is-network-error|p-defer|superjson)"
    ],
    "moduleNameMapper": {
      "^@exceptions": "<rootDir>/src/exceptions",
      "^@modules/(.*)$": "<rootDir>/src/modules/$1",
      "^@utils": "<rootDir>/src/utils",
      "^@typedefs": "<rootDir>/src/typedefs"
    },
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/apps/",
      "<rootDir>/src/"
    ]
